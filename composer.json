{
  "name": "sundata/php-utilities",
  "type": "library",
  "description": "Common php utilities library",
  "keywords": [
    "sundata",
    "php",
    "utilities",
    "common",
    "library"
  ],
  "homepage": "https://github.com/frittsnl/sundata-php-utilities",
  "authors": [
    {
      "name": "Maarten de Rie",
      "email": "maarten.de.rie@sundata.nl"
    }
  ],
  "require": {
    "php": "^7.4 || ^8",
    "nesbot/carbon": ">=2.64 || ^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7 || ^8",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Sundata\\Utilities\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sundata\\Utilities\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
